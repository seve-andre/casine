{
  "name": "casine",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "tauri": "tauri",
    "dev": "tauri dev",
    "dev:web": "vite",
    "build": "tauri build",
    "build:web": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "pnpm lint:frontend && pnpm lint:backend",
    "lint:frontend": "prettier --plugin-search-dir . --check . && eslint .",
    "lint:backend": "cd src-tauri && cargo clippy",
    "format": "pnpm format:frontend && pnpm format:backend",
    "format:frontend": "prettier --plugin-search-dir . --write .",
    "format:backend": "cd src-tauri && cargo fmt"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.6",
    "@sveltejs/adapter-static": "^1.0.6",
    "@sveltejs/kit": "^1.20.4",
    "@tauri-apps/cli": "1.4.0",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.7",
    "flowbite": "1.8.1",
    "flowbite-svelte": "0.43.1",
    "postcss": "^8.4.14",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.6.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.9",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-svelte": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0"
  },
  "dependencies": {
    "@tauri-apps/api": "1.3.0",
    "zod": "^3.21.4"
