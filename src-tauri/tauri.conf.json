{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../build"
  },
  "package": {
    "productName": "Casine",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "app": {
        "all": true,
        "show": true,
        "hide": true
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "identifier": "com.mitch.casine",
      "category": "Business",
      "shortDescription": "Gestionale per Le casine",
      "longDescription": "Si tratta di un gestionale per Le casine, che si occupa di affitti estivi di appartamenti lungo la riviera romagnola",
      "copyright": "MIT",
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "targets": "all",
      "deb": {
        "depends": []
      },
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "Le Casine",
        "height": 600,
        "width": 1024,
        "minHeight": 600,
        "minWidth": 1024,
        "center": true
      }
    ]
  }
}
