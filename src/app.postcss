/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

.title-text {
  font-weight: 700;
}

.btn {
  font-weight: 500;
  font-size: 0.875rem;
  line-height: 1.25rem;
  border-radius: 9999px;
  height: 2.5rem;
  padding: 0 1.5rem;

  display: flex;
  justify-content: center;
  align-items: center;
}
